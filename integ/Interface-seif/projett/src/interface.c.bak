  /* Store pointers to all widgets, for use by lookup_widget(). */
  GLADE_HOOKUP_OBJECT_NO_REF (windowHome, windowHome, "windowHome");
  GLADE_HOOKUP_OBJECT (windowHome, fixed28, "fixed28");
  GLADE_HOOKUP_OBJECT (windowHome, buttonInterfaceHsan, "buttonInterfaceHsan");
  GLADE_HOOKUP_OBJECT (windowHome, buttonInterfaceSeif, "buttonInterfaceSeif");

  return windowHome;
}

